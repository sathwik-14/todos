datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String
  updatedAt DateTime @updatedAt
}

model User {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  password  String
  updatedAt DateTime @updatedAt
  username  String   @unique
}
